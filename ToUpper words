using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace KodeWars2
{
    class Program
    {
        static void Main(string[] args)
        {
            string str = "sjhdjf";
            string str2 = "SDFSDF";
            Console.WriteLine(Upp(str));
            
        }
        public static bool Upp(string text)
        {
            return text == text.ToUpper();
        }
    }
}
